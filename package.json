{
  "name": "@jc-vendor/code-review",
  "version": "1.1.0",
  "description": "A comprehensive code review tool supporting multiple languages that integrates with husky hooks",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "code-review": "./dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "languages.yaml",
    "prompt-templates.yaml"
  ],
  "keywords": [
    "code-review",
    "husky",
    "lint",
    "javascript",
    "typescript",
    "java",
    "kotlin",
    "swift",
    "python",
    "git-hooks"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jc-vendor/code-review.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "glob": "^10.3.10",
    "minimatch": "^9.0.3",
    "commander": "^11.1.0",
    "fs-extra": "^11.1.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/glob": "^8.1.0",
    "@types/minimatch": "^5.1.2",
    "@types/fs-extra": "^11.0.3",
    "@types/js-yaml": "^4.0.9",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "@types/jest": "^29.5.6",
    "jest": "^29.7.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0",
    "ts-jest": "^29.1.1"
  }
